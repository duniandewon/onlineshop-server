{
  "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "warn",
    "no-unused-vars": "warn",
    "prefer-const": "warn",
    "no-console": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off"
  }
}
